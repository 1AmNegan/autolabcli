{
  "name": "autolabjs",
  "version": "1.0.0",
  "description": "Codebase for backward incompatible revision of autolabcli v0.1.1.",
  "main": "index.js",
  "scripts": {
    "posttest": "nyc report --reporter=lcov > coverage.lcov",
    "unit": "nyc --reporter=lcov --reporter=text-lcov -s --all mocha 'test/unit/**/*.js'",
    "post-unit": "nyc report --reporter=lcov > coverage.lcov && codecov && rm -rf ~/.autolabjs",
    "integration": "nyc --reporter=lcov --reporter=text-lcov -s --all mocha 'test/integration/**/*.js'",
    "post-integration": "CODECLIMATE_REPO_TOKEN=baebe9fb07d4b1cb32bae393a2eb1e66d8c8d1656b35db9d7d4c2eda3caf26ea codeclimate-test-reporter < ./coverage/lcov.info && rm -rf ~/.autolabjs",
    "feature": "nyc --reporter=lcov --reporter=text-lcov -s --all cucumber-js 'test/feature/features' -r 'test/feature/steps'",
    "post-feature": "cat ./coverage/lcov.info | coveralls"
  },
  "bin": {
    "autolabjs": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AutolabJS/autolabcli.git"
  },
  "author": "Sribalaji M",
  "license": "GPL v3.0",
  "bugs": {
    "url": "https://github.com/AutolabJS/autolabcli/issues"
  },
  "homepage": "https://github.com/AutolabJS/autolabcli#readme",
  "dependencies": {
    "caporal": "^0.9.0",
    "chalk": "^2.3.0",
    "cli-spinner": "^0.2.8",
    "cli-table": "^0.3.1",
    "figlet": "^1.2.0",
    "inquirer": "^5.0.0",
    "lodash": "^4.17.5",
    "preferences": "^1.0.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "cucumber": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^4.1.0",
    "mock-stdin": "^0.3.1",
    "nock": "^9.2.3",
    "nyc": "^11.4.1",
    "sinon": "^4.1.5",
    "sinon-chai": "^2.14.0"
  }
}
